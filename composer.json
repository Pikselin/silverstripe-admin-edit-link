{
    "name": "pikselin/silverstripe-admin-edit-link",
    "type": "silverstripe-vendormodule",
    "description": "Adds a link to the edit page in the CMS if you are logged in as administrator",
    "keywords": [
        "silverstripe",
        "admin"
    ],
    "homepage" : "https://github.com/Pikselin/silverstripe-admin-edit-link",
    "support" : {
      "issues" : "https://github.com/Pikselin/silverstripe-admin-edit-link/issues"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pikselin",
            "email": "info@pikselin.com",
            "homepage": "https://pikselin.com"
        }
    ],
    "require": {
        "silverstripe/cms": "^4 || ^5",
        "silverstripe/framework": "^4 || ^5"
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
